{
	"manifest_version": 3,
	"name": "My Shortcuts Tab",
	"version": "1.0",
	"description": "Add your most used shortcuts to your new tab",
	"permissions": [
		"bookmarks",
		"storage",
		"unlimitedStorage",
		"tabs",
		"activeTab"
	],
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "popup.html"
	},
	"icons": {
		"128": "icons/128-bw.png"
	}
}